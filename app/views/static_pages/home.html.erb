<% provide(:title, 'Sample App') %>

<% if signed_in? %>

	<section>
		<%= render 'static_pages/signed_in' %>
	</section>
	<section>
		<%= render 'shared/user_info' %>
	</section>
	<section>
		<%= render 'shared/stats' %>
	</section>
	<section>
		<%= render 'shared/micropost_form' %>
	</section>

<% else %>

  <%= render 'static_pages/not_signed_in' %>

<% end %>